
<div class="col-md-12 col-sm-12 col-xs-12 center login-form">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<div class="col-md-6 col-sm-8 col-xs-12 z-depth-4">


<div class="field">
    <%= f.label :email, :class => 'left' %><br />
    <%= f.email_field :email, autofocus: true, :required => true %>
</div>

<div class="field">
    <%= f.label :full_Name, :class => 'left' %><br />
    <%= f.text_field :full_name, :required => true%>
</div>

<div class="field">
    <%= f.label :profile_Picture, :class => 'left' %><br />
    <%= f.text_field :photo_url, :required => true%>
</div>

<div class="field">
    <%= f.label :password, :class => 'left' %>
    <% if @minimum_password_length %>
    <em class = "left"> (<%=  @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", :required => true %>
</div>

<div class="field">
    <%= f.label :password_confirmation, :class => 'left' %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", :required => true %>
</div>


<div class="actions">
    <%= f.submit "Sign up", :class => 'btn  waves-effect waves-light btn-custom' %>
</div>
<% end %>
</div>
<%= render "devise/shared/links" %>
</div>
