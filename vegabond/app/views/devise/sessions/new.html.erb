<div class="col-md-12 col-sm-12 col-xs-12 center login-form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="col-md-6 col-sm-8 col-xs-12 z-depth-4">
  <div class="field">
    <%= f.label :email, :class => 'left' %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password, :class => 'left' %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%# f.check_box :remember_me %>
      <%# f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions center">
    <%= f.submit "Sign In", :class => 'btn  waves-effect waves-light btn-custom' %>
  </div>
<% end %>
</div>
<%= render "devise/shared/links" %>

</div>

